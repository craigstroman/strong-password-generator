{
	"manifest_version": 2,

	"name": "Strong Password Generator",
	"description": "This extension will generator a strong password.",

	"version": "1.0.0",

	"permissions": [
	  "activeTab"
	],

	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"css": ["css/styles.css"],
		"js": ["js/main.js"],
		"all_frames": false
	}],

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	}
}